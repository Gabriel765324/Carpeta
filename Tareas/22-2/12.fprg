    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="e, c" type="Real" array="False" size=""/>
            <declare name="t" type="Integer" array="False" size=""/>
            <output expression="&quot;D&#233; la edad del paciente y d&#233; el tipo del paciente.&quot;" newline="True"/>
            <input variable="e"/>
            <input variable="t"/>
            <if expression="t &lt; 1 and t &gt; 4">
                <then>
                    <output expression="&quot;No es un tipo v&#225;lido.&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="t == 1">
