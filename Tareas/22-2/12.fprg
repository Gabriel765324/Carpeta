                                        </then>
                                        <else>
                                            <assign variable="c" expression="2000"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <if expression="e &lt; 15 or e &gt; 70">
                <then>
                    <assign variable="c" expression="c * 1.2"/>
                </then>
                <else/>
            </if>
            <output expression="&quot;El costo es de &quot;&amp;c&amp;&quot; bolivianos.&quot;" newline="True"/>
